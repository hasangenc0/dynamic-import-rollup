{
  "compilerOptions": {
    "lib": ["esnext", "dom", "dom.iterable"],
    "experimentalDecorators": true,
    "target": "es5",
    "downlevelIteration": true,
    "module": "esnext",
    "strict": true,
    "moduleResolution": "node",
    "composite": true,
    "declarationMap": true,
    "sourceMap": true,
    "importHelpers": true,
    "rootDir": ".",
    "outDir": ".ts"
  }
}